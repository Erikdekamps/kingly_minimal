{#
/**
 * @file
 * Theme override for a breadcrumb trail.
 *
 * This template acts as a bridge to the 'breadcrumbs' Single Directory Component.
 * It passes the necessary variables from the theme system to the component.
 *
 * @see kingly_minimal/components/breadcrumbs/breadcrumbs.twig
 */
#}
{% if breadcrumb %}
  {{ include('kingly_minimal:breadcrumbs', {
    breadcrumb: breadcrumb,
  }, with_context = false) }}
{% endif %}
