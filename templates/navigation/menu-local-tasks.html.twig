{#
/**
 * @file
 * Theme override to display primary and secondary local tasks.
 *
 * Available variables:
 * - primary: HTML list items representing primary tasks.
 * - secondary: HTML list items representing secondary tasks.
 *
 * @see template_preprocess_menu_local_tasks()
 */
#}
{% if primary %}
  <nav class="tabs-wrapper" role="navigation" aria-label="{{ 'Primary-tabs'|t }}">
    <h2 class="visually-hidden">{{ 'Primary-tabs'|t }}</h2>
    <ul class="local-tasks local-tasks--primary">{{ primary }}</ul>
  </nav>
{% endif %}
{% if secondary %}
  <nav class="tabs-wrapper" role="navigation" aria-label="{{ 'Secondary-tabs'|t }}">
    <h2 class="visually-hidden">{{ 'Secondary-tabs'|t }}</h2>
    <ul class="local-tasks local-tasks--secondary">{{ secondary }}</ul>
  </nav>
{% endif %}
