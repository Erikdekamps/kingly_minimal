{#
/**
 * @file
 * Theme override for the Accordion container paragraph.
 *
 * This template acts as a bridge, wrapping the rendered accordion items
 * in the 'kingly_minimal:accordion' Single Directory Component.
 *
 * @see kingly_minimal/components/accordion/accordion.twig
 */
#}
{% embed 'kingly_minimal:accordion' with {
  attributes: attributes,
} %}
  {% block content %}
    {{ content.field_accordion_items }}
  {% endblock %}
{% endembed %}
